public class recursion6 {
    public static int fibo(int n){
        if(n<2)
            return n;
        else{
            int fibn=fibo(n-1)+fibo(n-2);
            return fibn;
        }
    }
    public static void main(String[] args) {
        int n=5;
        System.out.println(fibo(n));
    }
}
