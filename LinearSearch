public class recursion7 {
    public static int recur(int arr[],int i,int key){
        if(i==arr.length-1)
            return -1;
        if(arr[i]==key)
            return i;
        return recur(arr,i+1,key);
    }
    public static void main(String args[]){
        int arr[]={8,3,6,9,2,5,10,5,3};
        int key=3;
        System.out.println(recur(arr,0,key));
    }
}
