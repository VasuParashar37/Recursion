class recursion1{
    public static void recurr(int n){
        if(n==0)
            return ;
        recurr(n-1);
        System.out.print(n+" ");
    }
    public static void recur(int n){
        if(n==1){
            System.out.println(n);
            return ;
        }
        System.out.print(n+" ");
        recur(n-1);
    }
    public static void main(String[] args) {
        int n=10;
        recur(n);
        recurr(n);
    }
}
